<!DOCTYPE html>
<!--[if IE 9]><html class="lt-ie10" lang="en" > <![endif]-->
<html class="no-js" lang="en">
    <head>
        <meta charset="utf-8">

        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>That's Catering!</title>

        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/foundation/6.1.0/foundation.min.css">
        <link rel="stylesheet" href="/css/app.css">
        <link href='http://cdnjs.cloudflare.com/ajax/libs/foundicons/3.0.0/foundation-icons.css' rel='stylesheet' type='text/css'>

        <!-- Grab Google CDN's jQuery. fall back to local if necessary -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js"></script>
    </head>
    <body>
        [% IF deferred.error_message || msgs.error_message %]
            [% IF msgs.error_message %][% message = msgs.error_message %][% ELSE %]
            [% message = deferred.error_message %][% END %]
        <div class="row">
            <div class="alert callout" data-closable>
                ERROR: [% message %]
                <button class="close-button" aria-label="Dismiss alert" type="button" data-close>
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        </div>
        [% END %]
        [% IF deferred.notify %]
        <div class="row">
            <div class="primary callout" data-closable>
                [% deferred.notify %]
                <button class="close-button" aria-label="Dismiss alert" type="button" data-close>
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        </div>
        [% END %]
        [% IF deferred.success %]
        <div class="row">
            <div class="success callout" data-closable>
                [% deferred.success %]
                <button class="close-button" aria-label="Dismiss alert" type="button" data-close>
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        </div>
        [% END %]
        <div class="top-bar">
            <div class="top-bar-left">
                <ul class="dropdown menu" data-dropdown-menu>
                    <li class="menu-text">
                        <a href="/"><img src="http://placehold.it/250x50&text=That's Catering!"></a></li>
                    <li class="has-submenu">
                        <a href="#">Browse Caterers</a>
                        <ul class="submenu menu vertical" data-submenu>
                            <li><a href="#">By Location</a></li>
                            <li><a href="#">By Cuisine</a></li>
                        </ul>
                    </li>
                    <li><a href="#">FAQ</a></li>
                    [% IF session.user %]
                    <li><a href="#">&middot;</a></li>
                    <li><a href="/account"><i class="fi-home"></i> [% session.user %]</a></li>
                    <li><a href="/logout"><i class="fi-power"></i> Log Out</a></li>
                    [% ELSE %]
                    <li><a href="/login">Sign In / Register</a></li>
                    [% END %]
                </ul>
            </div>
            <div class="top-bar-right">
                <form action="/search" method="post" style="display: inline;">
                <ul class="menu">
                    <li class="menu-text"><input type="search" placeholder="Search"></li>
                    <li class="menu-text"><input type="submit" value="Search" class="button"></li>
                </ul>
                </form>
            </div>
        </div>
        [% IF breadcrumbs %]
        <div class="row">
            <div class="small-12 columns">
                <nav aria-label="You are here:" role="navigation">
                    <ul class="breadcrumbs">
                        <li><a href="/">Home</a></li>
                        [% FOREACH crumb IN breadcrumbs %]
                        [% IF crumb.disabled %]
                        <li class="disabled">[% crumb.name %]</li>
                        [% ELSIF crumb.current %]
                        <li>
                            <span class="show-for-sr">Current: </span> [% crumb.name %]
                        </li>
                        [% ELSE %]
                        <li><a href="[% crumb.link %]">[% crumb.name %]</a></li>
                        [% END %]
                        [% END %]
                    </ul>
                </nav>
            </div>
        </div>
        [% END %]


        [% content %]


        <footer style="visibility:hidden;">
            <div class="row">
                <div class="small-12 columns">
                    <hr/>
                    <div class="row">
                        <div class="small-12 medium-4 columns">
                            <p>
                                &copy; 2016 <strong>That's Catering!</strong> &middot; All Rights Reserved<br>
                                <small>This product includes GeoLite2 data created by MaxMind, available from <a href="http://www.maxmind.com">http://www.maxmind.com</a>.</small>
                            </p>
                        </div>
                        <div class="small-12 medium-8 columns">
                            <div class="row">
                                <div class="small-3 columns text-center">
                                    <a href="/tos">Terms of Service</a>
                                </div>
                                <div class="small-3 columns text-center">
                                    <a href="/privacy">Privacy Policy</a>
                                </div>
                                <div class="small-3 columns text-center">
                                    <a href="/contact_us">Contact Us</a>
                                </div>
                                <div class="small-3 columns text-center">
                                    <a href="/about_us">About Us</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
        <script type="text/javascript">/* <![CDATA[ */
            !window.jQuery && document.write('<script type="text/javascript" src="http://localhost:3000/javascripts/jquery.js"><\/script>')
        /* ]]> */</script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/foundation/6.1.0/foundation.min.js"></script>
        <script>
            $(document).foundation();
        </script>

        <!-- post footer starts -->

        <script src="/javascripts/vendor/stickyFooter.js"></script>
        <script>
            // Your code goes here
        </script>
    </body>
</html>
